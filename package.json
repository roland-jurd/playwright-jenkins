{
  "name": "playwright-ts-jenkins",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "npx playwright test",
    "test-login": "npx playwright test login.spec.ts",
    "smoke-1": "PWTEST_BLOB_REPORT_NAME=smoke-1 PWTEST_BLOB_DO_NOT_REMOVE=1 npx playwright test --grep @smoke --grep-invert @test2 --reporter=blob",
    "smoke-2": "PWTEST_BLOB_REPORT_NAME=smoke-2 PWTEST_BLOB_DO_NOT_REMOVE=1 npx playwright test --grep @smoke --grep-invert @test1 --reporter=blob",
    "sanity-1": "PWTEST_BLOB_REPORT_NAME=sanity-1 PWTEST_BLOB_DO_NOT_REMOVE=1 npx playwright test --grep @sanity --grep-invert @test2 --reporter=blob",
    "sanity-2": "PWTEST_BLOB_REPORT_NAME=smoke-2 PWTEST_BLOB_DO_NOT_REMOVE=1 npx playwright test --grep @sanity --grep-invert @test1 --reporter=blob",
    "regression-1": "PWTEST_BLOB_REPORT_NAME=regression-1 PWTEST_BLOB_DO_NOT_REMOVE=1 npx playwright test --grep @regression --grep-invert @test2 --reporter=blob",
    "regression-2": "PWTEST_BLOB_REPORT_NAME=smoke-2 PWTEST_BLOB_DO_NOT_REMOVE=1 npx playwright test --grep @regression --grep-invert @test1 --reporter=blob",
    "eslint": "eslint src/**/* --ext .ts",
    "eslint-report": "eslint -f checkstyle src/**/* > eslint-report.xml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.36.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0"
  }
}
